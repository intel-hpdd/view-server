{
  "name": "intel-view-server",
  "version": "3.0.4",
  "description": "Serves the HTML pages for the IML GUI.",
  "main": "index.js",
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs ."
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "author": "IML Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/view-server.git"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "dependencies": {
    "bunyan": "1.3.4",
    "highland": "2.4.0",
    "intel-fp": "1.0.13",
    "intel-req": "1.0.5",
    "intel-router": "3.0.3",
    "intel-through": "1.0.7",
    "lodash": "3.10.1",
    "nconf": "0.7.1",
    "xmlrpc": "1.3.0"
  },
  "devDependencies": {
    "jasmine": "2.3.1",
    "jasmine-n-matchers": "2.0.2",
    "jscs": "2.1.0",
    "jshint": "2.8.0",
    "krusty-jasmine-reporter": "1.0.0",
    "pre-commit": "1.1.1",
    "proxyquire": "1.5.0"
  },
  "whitelist": [
    "jasmine",
    "jasmine-n-matchers",
    "krusty-jasmine-reporter",
    "proxyquire"
  ]
}
