  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "@intel-js/fp": "1.0.2",
    "@intel-js/lodash-mixins": "1.0.0",
    "@intel-js/req": "1.0.1",
    "@intel-js/router": "3.0.1",
    "@intel-js/through": "1.0.4",
    "bunyan": "1.3.4",
    "highland": "2.4.0",
    "nconf": "0.7.1",
    "xmlrpc": "1.3.0"
  },
  "devDependencies": {
    "jasmine": "2.3.1",
    "jasmine-n-matchers": "2.0.2",
    "jscs": "2.1.0",
    "jshint": "2.8.0",
    "krusty-jasmine-reporter": "1.0.0",
    "pre-commit": "1.1.1",
    "proxyquire": "1.5.0"
  },
  "whitelist": ["jasmine", "jasmine-n-matchers", "krusty-jasmine-reporter", "proxyquire"]
