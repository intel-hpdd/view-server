{
  "name": "@intel-js/view-server",
  "version": "3.0.1",
  "description": "Serves the HTML pages for the IML GUI.",
  "main": "index.js",
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
